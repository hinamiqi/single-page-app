{
    "compilerOptions": {
        "module": "CommonJS",
        // "module":"ES6",
        "esModuleInterop": true,
        "target": "ES6",
        "noImplicitAny": true,
        "noImplicitReturns": false,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "*": [
                "node_modules/*"
            ]
        }
    },
    "include": [
        "src/**/*"
    ]
}